    "import heapq\n",
    "\n",
    "def solution(scoville, K):\n",
    "    i=0\n",
    "    heapq.heapify(scoville)\n",
    "    while True:\n",
    "        min1 = heapq.heappop(scoville)\n",
    "        if min1 >= K:\n",
    "            return i\n",
    "        elif len(scoville)==0:\n",
    "            return -1\n",
    "        else:\n",
    "            min2 = heapq.heappop(scoville)\n",
